{
  "name": "fotoboek",
  "version": "1.0.0-alpha",
  "description": "Simple photo album builder",
  "scripts": {
    "prebuild": "rimraf dist && mkdirp dist",
    "build": "run-s build:*",
    "build:css": "postcss src/index.css --output dist/index.css --no-map",
    "dev": "run-p dev:*",
    "dev:serve": "nodemon index.js",
    "dev:watch": "run-p watch:*",
    "start": "node index.js",
    "watch:css": "postcss src/index.css --output dist/index.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phortuin/fotoboek.git"
  },
  "author": "Anne Fortuin",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "_moduleAliases": {
    "@lib": "lib",
    "@models": "app/models",
    "@routes": "app/routes"
  },
  "bugs": {
    "url": "https://github.com/phortuin/fotoboek/issues"
  },
  "dependencies": {
    "bluebird": "3.5.2",
    "body-parser": "1.18.3",
    "cloudinary": "1.11.0",
    "dotenv-safe": "6.1.0",
    "express": "4.16.4",
    "express-mongo-sanitize": "1.3.2",
    "marked": "0.5.1",
    "mkdirp": "0.5.1",
    "module-alias": "2.1.0",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "mongoose": "5.3.4",
    "multer": "1.4.1",
    "npm-run-all": "4.1.3",
    "nunjucks": "3.1.3",
    "postcss-cli": "6.0.1",
    "postcss-custom-properties": "8.0.8",
    "postcss-import": "12.0.0",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  }
}
